        <dc:title>Lateral Thinking</dc:title>
        <dc:creator opf:file-as="Bono, Edward De" opf:role="aut">Edward De Bono</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
