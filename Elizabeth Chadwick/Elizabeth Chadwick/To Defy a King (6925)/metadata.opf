        <dc:title>To Defy a King</dc:title>
        <dc:creator opf:file-as="Chadwick, Elizabeth" opf:role="aut">Elizabeth Chadwick</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
